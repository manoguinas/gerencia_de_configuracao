{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["main.js"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "verbose": true
  },
  "plugins": []
}
